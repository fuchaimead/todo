<h1>To Do</h1>

<h2> <%= @list.title %> </h2>
<hr />
<strong> 
Complete: <%= @list.complete ? 'Yes' : 'No' %>
</strong>
<br />
<i>Created At: <%= @list.created_at %>
</i>
<br />
<i> Updated At: <%= @list.updated_at %>
</i>

<div class='action-buttons'>
  <%= link_to 'Edit', edit_list_path(@list)%>
  <%= link_to 'Delete', list_path(@list), method: :delete, data: {confirm: 'Are you sure you want to delete?'} %>
  <%= link_to 'All To Dos', lists_path %>
</div>
